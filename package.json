{
  "type": "module",
  "name": "jokko-health-backend",
  "version": "1.0.0",
  "description": "Backend complet pour l'application JokkoHealth avec authentification, CRUD, emails et validation",
  "main": "src/app.js",
  "scripts": {
    "start": "node_modules/.bin/babel-node src/app.js",
    "dev": "node_modules/.bin/babel-node src/app.js",
    "prod": "node src/app.js",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test jest --config jest.e2e.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "db:seed": "node scripts/seed.js",
    "db:migrate": "node scripts/migrate.js",
    "doc": "jsdoc -c jsdoc.config.json",
    "build:swagger": "node src/docs/swagger/generateSpec.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "agenda": "^5.0.0",
    "apidoc": "^0.29.0",
    "aws-sdk": "^2.1498.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bull": "^4.11.3",
    "cache-manager": "^5.2.4",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "email-templates": "^11.1.1",
    "express": "^4.21.2",
    "express-actuator": "^1.8.4",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.0",
    "express-status-monitor": "^1.2.3",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "i18n": "^0.15.1",
    "i18next": "^23.7.6",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jokko-health-backend": "file:",
    "jsonwebtoken": "^9.0.2",
    "mjml": "^4.7.1",
    "mongoose": "^8.10.1",
    "mongoose-autopopulate": "^1.0.1",
    "mongoose-delete": "^1.0.1",
    "mongoose-history": "^0.8.0",
    "mongoose-paginate-v2": "^1.7.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pino": "^8.16.2",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.9",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "faker": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "lint-staged": "^15.1.0",
    "mock-aws-s3": "^4.0.2",
    "mongodb-memory-server": "^9.1.1",
    "nock": "^13.3.8",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.3",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "health",
    "backend",
    "api",
    "express",
    "mongodb",
    "nodejs"
  ],
  "author": "JokkoHealth Team",
  "license": "MIT"
}
